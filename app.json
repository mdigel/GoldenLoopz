{
  "expo": {
    "name": "Golden Loopz",
    "slug": "golden-loopz",
    "scheme": "goldenloopz",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFBEB"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.goldenloopz.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFBEB"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.goldenloopz.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a22cab2e-d70f-45bc-b808-434dbea3b479"
      }
    },
    "owner": "mdigel"
  }
}
